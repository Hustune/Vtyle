#centralWidget{
    background:rgb(44,49,60);
}

#TextEdit {
    background: rgb(44,49,60);
    color: white;
    font-size: 50px;
}

#completer{
    color:white;
    background-color:rgb(33,37,43);
    font-size:30px;
}


#mainMenuBar{
    background:#000;
    color:#FFF;
    font-size:23px;
}
#mainMenuBar::item{
    background-color:rgb(33,37,43);
}
#mainMenuBar::item:selected{
    background-color:rgb(50,56,68);
}

#windowMenu{
    color:#FFF;
    border:none;
}
#windowMenu::item{
    background-color:rgb(33,37,43);
}
#windowMenu::item:selected{
    background-color:rgb(50,56,68);
}

#windowToolBar{
    background-color: rgb(44,49,60);
}


#tabWidget{
    border:10px;
    background-color:rgb(44,49,60);
}
#tabWidget::pane{
    background-color:rgb(44,49,60);
	border:none;
}
#tabBar::tab:hover{
    color:#FFF;
	background:rgb(50,56,68);
}
#tabBar::tab{
    background-color:rgb(33,37,43);
    color:white;
        font-size:22px;
}
#tabBar::tab:selected{
    background:#000;
    }
#tabBar::close-button{
    border-image: url(./Resource/TabCloseButton.png);
}
#tabBar::close-button:hover{
    background: rgb(100,148,237);
}


#dockWidget{
    background: white;
    padding:none;
}

#introduceLabel{
    color:white;
    font-size:30px;
}

#replaceBox{
    background-color:rgb(33,37,43);
    color:white;
    font-size:35px;
}
#replaceBox QAbstractItemView::item{
	height:36px;
	color:#FFF;
	padding-left:9px;
	background-color:rgb(33,37,43);
}
#replaceBox QAbstractItemView::item:hover{
  background-color:rgb(61,66,75);
  color:#FFF;
}
#replaceBox QAbstractItemView::item:selected{
  background-color:black;
  color:#FFF;
}
#replaceBox QScrollBar::vertical {
background:transparent;
width:8px;
border:0px;
}

#replaceBox QScrollBar::handle:vertical {
background:#6eb1f0;
width:8px;
}

#replaceBox QScrollBar::add-line:vertical {
background:none;
border:none;
}

#replaceBox QScrollBar::sub-line:vertical {
background:none;
border:none;
}

#replaceBox QScrollBar::add-page:vertical {
background:transparent;
border:none;
}

#replaceBox QScrollBar::sub-page:vertical {
background:transparent;
border:none;
}

